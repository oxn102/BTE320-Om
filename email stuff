      "cell_type": "code",
      "source": [
        "email = \"URGENT!! Click on the link: www.phis.org and receive a reward!\"\n",
        "has_urgent = \"URGENT\"\n",
        "has_link = \"www.\"\n",
        "\n",
        "if has_urgent in email and has_link in email:\n",
        "  print(\"Phishing!\")\n",
        "elif has_urgent in email and not has_link in email:\n",
        "  print(\"Spam!\")\n",
        "elif not has_urgent in email and not has_link in email:\n",
        "  print(\"Legit!\")"
      ],
      "metadata": {
